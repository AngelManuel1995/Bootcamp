<p id="notice"><%= notice %></p>

<h1>Listing Game Teams</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Team</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_teams.each do |game_team| %>
      <tr>
        <td><%= game_team.game_id %></td>
        <td><%= game_team.team_id %></td>
        <td><%= game_team.result %></td>
        <td><%= link_to 'Show', game_team %></td>
        <td><%= link_to 'Edit', edit_game_team_path(game_team) %></td>
        <td><%= link_to 'Destroy', game_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game team', new_game_team_path %>
